<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sdnmonitor.ransomer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SDNEventListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SDNEventDetailActivity"
            android:label="@string/title_sdnevent_detail"
            android:parentActivityName=".SDNEventListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SDNEventListActivity" />
        </activity>
    </application>

</manifest>
